# Contentful Configuration (Backend - for scripts)
CONTENTFUL_SPACE_ID=your_space_id_here
CONTENTFUL_ACCESS_TOKEN=your_access_token_here
CONTENTFUL_ENVIRONMENT=master

# Contentful Management API (for pushing content models)
CONTENTFUL_MANAGEMENT_TOKEN=your_management_token_here

# Optional: Contentful Preview API (for draft content)
CONTENTFUL_PREVIEW_ACCESS_TOKEN=your_preview_access_token_here

# Frontend Environment Variables (Vite requires VITE_ prefix)
VITE_CONTENTFUL_SPACE_ID=your_space_id_here
VITE_CONTENTFUL_ACCESS_TOKEN=your_access_token_here
VITE_CONTENTFUL_ENVIRONMENT=master

# Site Configuration (Frontend)
VITE_SITE_URL=https://rotaryzcwest.org
VITE_SITE_NAME="Rotary Club of Zamboanga City West" 

# PostHog Analytics Configuration (Frontend)
VITE_POSTHOG_KEY=your_posthog_api_key_here
VITE_POSTHOG_HOST=https://app.posthog.com